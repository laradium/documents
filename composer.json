{
    "name": "laradium/laradium-documents",
    "keywords": [
        "laradium",
        "document",
        "invoice",
        "contract",
        "pdf"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Artūrs Lukjaņenoks",
            "email": "arturs.lukjanenoks@netcore.lv"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.2",
        "barryvdh/laravel-dompdf": "^0.8.4"
    },
    "require-dev": {
        "orchestra/testbench": "~3.4.2|~3.5.0|~3.6.0|~3.7.0|~3.8.0",
        "phpunit/phpunit": "^5.7|6.2|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Laradium\\Laradium\\Documents\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laradium\\Laradium\\Documents\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laradium\\Laradium\\Documents\\Providers\\LaradiumDocumentServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/laradium/laradium.git"
        }
    ]
}
